apple_id(ENV['OWNCLOUD_APPLE_ID']) # Your Apple email address

itc_team_id(ENV['OWNCLOUD_ITC_TEAM']) # iTunes Connect Team ID
team_id(ENV['OWNCLOUD_TEAM_ID']) # Developer Portal Team ID

